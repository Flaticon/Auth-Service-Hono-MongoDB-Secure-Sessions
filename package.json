{
  "name": "auth-service-typescript",
  "version": "1.0.0",
  "description": "Servicio completo de autenticaciÃ³n con TypeScript, Hono y MongoDB",
  "main": "dist/server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "echo \"No tests specified yet\" && exit 0",
    "db:seed": "node dist/scripts/seed.js",
    "db:migrate": "node dist/scripts/migrate.js"
  },
  "keywords": [
    "auth",
    "authentication",
    "authorization",
    "typescript",
    "hono",
    "mongodb",
    "jwt",
    "session",
    "security"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.0.0",
    "@hono/node-server": "^1.8.0",
    "mongodb": "^6.3.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "validator": "^13.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "ts-node-dev": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/validator": "^13.11.7",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prettier": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/auth-service-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/auth-service-typescript/issues"
  },
  "homepage": "https://github.com/tu-usuario/auth-service-typescript#readme"
}